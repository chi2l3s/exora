{
  "name": "exora-sdk",
  "version": "1.0.0",
  "description": "Official Exora SDK for Node.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "jest"
  },
  "keywords": [
    "exora",
    "payments",
    "sdk",
    "fintech",
    "api"
  ],
  "author": "Exora Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@nestjs/common": ">=9.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    }
  }
}
